<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:ToDo.Converters">

    <converters:FromBooleanToDoubleConverter x:Key="TrueToWeakOpacity" />
    <converters:FromEnumerableAnyToVisibilityConverter x:Key="FromAnyToVisibileConverter" IsAnyVisible="True" />
    <converters:FromEnumerableAnyToVisibilityConverter x:Key="FromAnyToInvisibileConverter" IsAnyVisible="False" />
    <converters:FromEnumerableToCountConverter x:Key="FromEnumerableToCountConverter" />
    <converters:FromStateItemsRemainingToProgressConverter x:Key="FromStateItemsRemainingToProgressConverter" />
    <converters:FromNumberToItemsLeftConverter x:Key="FromNumberToItemsLeftConverter" />

</ResourceDictionary>